{% extends '::base.html.twig' %}
{% block titulo %}
    <h1 class="page-title"> Cargar Estudiantes </h1>
{% endblock %}
{% block body %}
  <h3>Cargar Archivo </h3>
  <form role="form" method="post" enctype="multipart/form-data" action='{{path('showarchivo')}}'>
      <div class="input-group">
          <label class="sr-only">Archivo</label>
          <input type="file" name="archivo" class="form-control" />
          <p class="help-block">Formatos adminitidos (.xls .xlsx .ods .csv .xlt)</p>
    </div>
    <button type="submit" class="btn btn-default" >Subir</button>
  </form>
  <hr/>
{% endblock %}
