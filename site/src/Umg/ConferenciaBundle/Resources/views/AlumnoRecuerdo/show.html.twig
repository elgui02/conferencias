{% extends '::base.html.twig' %}

{% block body -%}
    <h1>AlumnoRecuerdo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fechahora</th>
                <td>{{ entity.FechaHora|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Alumno_id</th>
                <td>{{ entity.Alumnoid }}</td>
            </tr>
            <tr>
                <th>Recuerdo_id</th>
                <td>{{ entity.Recuerdoid }}</td>
            </tr>
            <tr>
                <th>Usuario_id</th>
                <td>{{ entity.Usuarioid }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('alumnorecuerdo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('alumnorecuerdo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
